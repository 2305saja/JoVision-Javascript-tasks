<!DOCTYPE html>
<html>
<head>
</head>
<body>

  <button id="EVEN_Button">Click me</button>

  <script>
    document.getElementById("EVEN_Button").addEventListener("click", function() {
      const numbers = [];
      
      for (let i = 0; i <= 100; i++) {
        numbers.push(i);
      }


      let evenSum = 0;
      for (let i = 0; i < numbers.length; i++) {
        if (numbers[i] % 2 === 0) {
          evenSum += numbers[i];
        }
      }

  
      console.log("Sum of EVEN numbers:", evenSum);
    });
  </script>

</body>
</html>
